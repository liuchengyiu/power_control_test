{
    "Stress_Test":[
        {
            "type":"cpu_stress",
            "param":{
                "time":20
            }
        },
        {
            "type":"ram_stress",
            "param":{
                "num": 10,
                "size": "1G",
                "release_time": "10",
                "last_time": 20
            }
        }
    ],
    "Performance_Test":[
        {
            "type":"cpu_frequence",
            "param":{
                "average": 100,
                "single": 100
            }
        },
        {
            "type":"emmc_speed",
            "param":{
                "speed": 0.02
            }
        },
        {
            "type":"ram_speed",
            "param":{
                "speed": 1.4
            }
        },
        {
            "type":"rtc_time",
            "param":{
                "time": 5
            }
        }
    ],
    "Hardware_Test":[
        {
            "type":"card_4g_at_test",
            "param":{
                "port": "/dev/ttyACM1",
                "baudrate": 115200
            }
        },
        {
            "type":"eth_speed",
            "param":{
                "serverip":"192.168.1.115",
                "speed":100
            }
        },
        {
            "type":"jc_test",
            "param":{
                "ip":"127.0.0.1",
                "port": 1883,
                "interval": 1,
                "times": 4
            }
        },

        {
            "type":"key_test",
            "param":{
                "ip":"127.0.0.1",
                "port": 1883,
                "interval": 3,
                "times": 4,
                "gpio": 200
            }
        },
        {
            "type":"test_485",
            "param":{
                "serial_send":"/dev/ttyACM1",
                "serial_read":"/dev/ttyACM4",
                "time": 5,
                "time_01": 1
            }
        },
        {
            "type":"relay_test",
            "param":{
                "serial_relay":"/dev/ttyACM0",
                "serial_relay2":"/dev/ttyACM1",
                "time": 1
            }
        }
    ]
}
